{
    "name": "otisz/laravel-imgix",
    "description": "Laravel package for generating Imgix URLs for your images",
    "keywords": [
        "laravel",
        "laravel-5",
        "laravel-6",
        "laravel-7",
        "laravel-5-package",
        "laravel-6-package",
        "laravel-7-package",
        "imgix",
        "cdn",
        "image",
        "image-processing"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Otisz/Laravel-Imgix/issues",
        "source": "https://github.com/Otisz/Laravel-Imgix"
    },
    "authors": [
        {
            "name": "Levente Otta",
            "email": "leventeotta@gmail.com",
            "homepage": "https://leventeotta.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "imgix/imgix-php": "^3.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.2|2.*",
        "orchestra/testbench": "^3.5|^4.0|^5.0",
        "phpunit/phpunit": "^6.5|^7.0|^8.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Otisz\\Imgix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Otisz\\Imgix\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Otisz\\Imgix\\ImgixServiceProvider"
            ],
            "aliases": {
                "Imgix": "Otisz\\Imgix\\Facades\\Imgix"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
